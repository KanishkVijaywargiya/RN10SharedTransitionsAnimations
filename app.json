{
  "expo": {
    "name": "10 React Native",
    "slug": "10-react-native",
    "description": "10 React Native Shared Element transition challenges",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.BatmanCodes.10ReactNative",
      "buildNumber": "4",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.BatmanCodes.TENReactNative",
      "permissions": ["CAMERA_ROLL", "CAMERA", "NOTIFICATIONS"],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
